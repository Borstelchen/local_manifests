<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <!-- Samsung general -->
  <project name="LineageOS/android_hardware_samsung" path="hardware/samsung" remote="github" />
  <project name="LineageOS/android_hardware_samsung_slsi_exynos" path="hardware/samsung_slsi/exynos" remote="github" />
  <project name="LineageOS/android_hardware_samsung_slsi_exynos5" path="hardware/samsung_slsi/exynos5" remote="github" />
  <project name="LineageOS/android_hardware_samsung_slsi_openmax" path="hardware/samsung_slsi/openmax" remote="github" />
  <!-- patches to soong - the new Android build system -->
  <remove-project name="LineageOS/android_build_soong" />
  <project name="Borstelchen/android_build_soong" path="build/soong" remote="github" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>
  <!-- Override LineageOS's Updater package -->
  <remove-project name="LineageOS/android_packages_apps_Updater" />
  <project name="Borstelchen/android_packages_apps_Updater" path="packages/apps/Updater" remote="github" />
  <!-- Samsung Galaxy S6 (SM-G92{0/5}F variants) - zero(f)ltexx -->
  <project name="Borstelchen/android_device_samsung_zeroltexx" path="device/samsung/zeroltexx" remote="github" />
  <project name="Borstelchen/android_device_samsung_zerofltexx" path="device/samsung/zerofltexx" remote="github" />
  <project name="Borstelchen/android_device_samsung_universal7420-common" path="device/samsung/universal7420-common" remote="github" />
  <project name="Borstelchen/android_vendor_samsung_zeroltexx" path="vendor/samsung/zeroltexx" remote="github" />
  <project name="Borstelchen/android_vendor_samsung_zerofltexx" path="vendor/samsung/zerofltexx" remote="github" />
  <project name="Borstelchen/android_vendor_samsung_universal7420-common" path="vendor/samsung/universal7420-common" remote="github" />
  <project name="Borstelchen/android_kernel_samsung_universal7420" path="kernel/samsung/universal7420" remote="github" />
  <project name="Borstelchen/android_hardware_samsung_slsi_exynos7420" path="hardware/samsung_slsi/exynos7420" remote="github" />
</manifest>
